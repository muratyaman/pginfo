  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/pg": "^8.6.5",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
